# MORPHEUS Environment Configuration
# Copy this file to .env and update values for your environment

# Database Configuration
DATABASE_HOST=localhost
DATABASE_PORT=5432
DATABASE_NAME=morpheus
DATABASE_USER=morpheus_user
DATABASE_PASSWORD=morpheus_pass

# GASM Integration
GASM_ROBOTING_PATH=./GASM-Robotics
GASM_ENABLED=true

# System Configuration
MORPHEUS_MODE=full
MORPHEUS_DEVICE=cpu
MORPHEUS_LOG_LEVEL=INFO

# Performance Settings
MORPHEUS_MAX_MEMORY_GB=4
MORPHEUS_PARALLEL_DREAMS=4
MORPHEUS_BATCH_SIZE=32

# Development Settings
MORPHEUS_DEBUG=false
MORPHEUS_PROFILING=false